          <level>0</level>
          <default>default</default>
          <constraints>
            <options>
              <option label="30176">default</option>
              <option label="30178">fixed-res</option>
              <option label="30179">ask-quality</option>
              <option label="30177">manual-osd</option>
              <option label="30180">test</option>
